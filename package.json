{
  "name": "chmapp",
  "version": "0.0.1",
  "description": "Under development...",
  "repository": {
    "type": "git",
    "url": "https://github.com/e-freak/chmapp"
  },
  "main": "app/js/MainProcess/main.js",
  "scripts": {
    "build": "gulp transpile_js",
    "start": "electron .",
    "build_test": "gulp transpile_test",
    "test": "mocha test/mocha-test.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "latest",
    "babel-core": "latest",
    "babel-preset-env": "latest",
    "eslint": "latest",
    "gulp": "latest",
    "gulp-babel": "latest",
    "gulp-load-plugins": "latest",
    "htmlhint": "latest",
    "istanbul": "latest",
    "mocha": "latest",
    "nyc": "latest"
  },
  "dependencies": {
    "electron": "latest"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    },
    "comments": false
  },
  "nyc": {
    "include": [
      "src/es6/lib/**/*js"
    ],
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
